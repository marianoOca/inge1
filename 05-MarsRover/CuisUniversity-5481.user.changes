!MarsRoverTest methodsFor: 'tests' stamp: 'cde 10/6/2022 13:05:27'!
test26ReceivingAnInvalidStringRaisesErrorAndDoesNotContinueMovingRover

	| marsRover instructionFromEarth|
	
	instructionFromEarth := 'ffjf'.				
	marsRover := MarsRover withPosition: (0@0) andDirection: #N.
	
	self 
		should: [ marsRover receiveInstructionString: instructionFromEarth ] 
		raise: Error  
		withExceptionDo: [ :error | 
			self assert: (marsRover isInPosition: (0@2) andDirection: #N).
			self assert: error messageText equals: 'Invalid instruction!! Execution stopped.'. ]! !

!methodRemoval: MarsRoverTest #test27ReceivingAnInvalidStringRaisesErrorAndDoesNotContinueMovingRover stamp: 'cde 10/6/2022 13:05:30'!
test27ReceivingAnInvalidStringRaisesErrorAndDoesNotContinueMovingRover

	| marsRover instructionFromEarth|
	
	instructionFromEarth := 'ffjf'.				
	marsRover := MarsRover withPosition: (0@0) andDirection: #N.
	
	self 
		should: [ marsRover receiveSingleInstruction: instructionFromEarth ] 
		raise: Error  
		withExceptionDo: [ :error | 
			self assert: (marsRover isInPosition: (0@2) andDirection: #N).
			self assert: error messageText equals: 'Invalid instruction!! Execution stopped.'. ]!

